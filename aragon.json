{
  "name": "Aragon Token Distribution",
  "map": {
    "item": {
      "address": "ethereumAddress",
      "amount": "scoreRealized",
      "tokenName": ""
    },
    "operate": [
      {
        "run": "(item.amount / totalPraiseScore) * budget",
        "on": "amount"
      }
    ],
    "each": "(function each(item, context) { item.tokenName = context.token; return item; })(item, context)"
  },
  "context": {
    "budget": "number",
    "token": "string"
  }
}
