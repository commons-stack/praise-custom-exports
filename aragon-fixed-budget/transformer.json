{
  "name": "Aragon fixed budget",
  "map": {
    "item": {
      "address": "ethereumAddress",
      "amount": "scoreRealized",
      "token": ""
    },
    "operate": [
      {
        "run": "(item.amount / totalPraiseScore) * budget",
        "on": "amount"
      },
      {
        "run": "token",
        "on": "token"
      }
    ],
    "each": "item"
  },
  "context": {
    "budget": "number",
    "token": "string"
  },
  "filterColumn": "address",
  "includeCsvHeaderRow": false
}
